      "name": "A2C",
      "algorithm": {
        "name": "ActorCritic",
        "action_pdtype": "default",
        "action_policy": "default",
        "explore_var_spec": null,
        "gamma": 0.95,
        "use_gae": true,
        "lam": 0.97,
        "use_nstep": false,
